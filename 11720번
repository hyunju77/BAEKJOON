#include <stdio.h>

int main() {
    int num, sum=0;
    int n_in[100]={0};
    
    scanf("%d\n", &num);
    
    for (int i=0; i<num; i++) {
        scanf("%d", &n_in[i]);
        if (n_in[i] == 0) break;
    }
    
    for (int i=0; i<num; i++) {
        sum += n_in[i];
    }
    printf("%d\n", sum);
    return 0;
}
